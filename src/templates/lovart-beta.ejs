<!DOCTYPE html>
<% const breadcrumbItems=[{ name: t.nav.home, href: link('/') }, { name: 'Lovart Beta' , href: canonicalUrl }]; %>
    <html lang="<%= lang %>" class="scroll-smooth">

    <head>
        <%- include('partials/head') %>
            <meta charset="UTF-8">
            <meta name="viewport" content="width=device-width, initial-scale=1.0">
            <title>
                <%= t.seo.lovart_beta.title %>
            </title>
            <meta name="description" content="<%= t.seo.lovart_beta.description %>">
            <meta name="keywords" content="">
            <link rel="canonical" href="<%= canonicalUrl %>">
            <link rel="stylesheet" href="/styles.css">
            <link rel="icon" href="/favicon.ico" type="image/x-icon">
            <script async
                src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-3083296102953963"
                crossorigin="anonymous"></script>
            <%- include('partials/analytics') %>
                <%- include('partials/schema-breadcrumb', { breadcrumbItems }) %>
                    <%- include('partials/hreflang') %>
                        <script type="application/ld+json"><%- JSON.stringify({
        "@context": "https://schema.org",
        "@type": "FAQPage",
        "mainEntity": t.lovart_beta_page.faq.items.map(function(f) {
            return { "@type": "Question", "name": f.q, "acceptedAnswer": { "@type": "Answer", "text": f.a } };
        })
    }) %></script>
                        <style>
                            .hero-beta {
                                background: linear-gradient(135deg, #0c1a0c 0%, #14532d 50%, #0c1a0c 100%);
                            }

                            .timeline-wrap {
                                border-left: 3px solid #22c55e;
                                padding-left: 1.25rem;
                            }

                            .tl-dot {
                                width: 0.875rem;
                                height: 0.875rem;
                                border-radius: 50%;
                                margin-left: -1.7rem;
                                margin-right: 0.75rem;
                                flex-shrink: 0;
                                margin-top: 0.25rem;
                            }

                            .tl-dot-future {
                                background: #06b6d4;
                                animation: pulse 1.5s ease-in-out infinite;
                            }

                            @keyframes pulse {

                                0%,
                                100% {
                                    opacity: 1;
                                    transform: scale(1)
                                }

                                50% {
                                    opacity: .4;
                                    transform: scale(.7)
                                }
                            }

                            .compare-table {
                                width: 100%;
                                border-collapse: collapse;
                            }

                            .compare-table th,
                            .compare-table td {
                                padding: 0.6rem 0.85rem;
                                font-size: 0.82rem;
                                border-bottom: 1px solid #e5e7eb;
                            }

                            .compare-table th {
                                background: #f0fdf4;
                                font-weight: 700;
                                text-align: center;
                            }

                            .compare-table td:first-child {
                                font-weight: 600;
                                color: #374151;
                            }

                            .compare-table td:nth-child(2) {
                                color: #9ca3af;
                                text-align: center;
                            }

                            .compare-table td:nth-child(3) {
                                color: #166534;
                                text-align: center;
                                font-weight: 500;
                            }

                            .compare-table tr:last-child td {
                                border-bottom: none;
                            }

                            details summary {
                                cursor: pointer;
                                list-style: none;
                            }

                            details summary::-webkit-details-marker {
                                display: none;
                            }

                            .chevron {
                                transition: transform 0.2s;
                            }

                            details[open] .chevron {
                                transform: rotate(180deg);
                            }
                        </style>
    </head>

    <body class="bg-neutral-light text-neutral-dark antialiased">
        <%- include('partials/header') %>
            <%- include('partials/breadcrumb', { breadcrumbItems }) %>
                <main>

                    <!-- â•â•â• HERO â•â•â• -->
                    <section class="hero-beta py-16 md:py-24">
                        <div class="container mx-auto px-4 max-w-4xl text-center">
                            <span
                                class="inline-block py-1 px-4 rounded-full bg-green-400/15 text-green-300 text-sm font-semibold mb-5 border border-green-400/30">
                                <%= t.lovart_beta_page.hero.badge %>
                            </span>
                            <h1 class="text-3xl md:text-4xl lg:text-5xl font-bold text-white mb-5 leading-tight">
                                <%= t.lovart_beta_page.hero.title %>
                            </h1>
                            <p class="text-base md:text-lg text-green-200 mb-8 max-w-2xl mx-auto leading-relaxed">
                                <%= t.lovart_beta_page.hero.subtitle %>
                            </p>
                            <div class="flex flex-col sm:flex-row gap-3 justify-center">
                                <a href="<%= link('lovart-nano-banana') %>"
                                    class="inline-flex items-center justify-center px-7 py-3.5 bg-green-500 text-white font-bold rounded-xl shadow-lg hover:bg-green-400 transition-all text-base">
                                    ðŸš€ <%= t.lovart_beta_page.hero.cta_primary %> â†’
                                </a>
                                <a href="<%= link('lovart-free') %>"
                                    class="inline-flex items-center justify-center px-7 py-3.5 bg-white/10 text-white font-semibold rounded-xl border border-white/20 hover:bg-white/20 transition-all text-base">
                                    <%= t.lovart_beta_page.hero.cta_secondary %>
                                </a>
                            </div>
                        </div>
                    </section>

                    <!-- AdSense 1 -->
                    <div class="container mx-auto px-4 max-w-4xl my-6">
                        <%- include('partials/adsense', { format: 'horizontal' }) %>
                    </div>

                    <!-- â•â•â• TIMELINE â•â•â• -->
                    <section class="bg-white py-14">
                        <div class="container mx-auto px-4 max-w-3xl">
                            <h2 class="text-2xl md:text-3xl font-bold text-neutral-dark mb-2 text-center">
                                <%= t.lovart_beta_page.timeline.heading %>
                            </h2>
                            <p class="text-neutral text-center mb-10 text-sm leading-relaxed max-w-xl mx-auto">
                                <%= t.lovart_beta_page.timeline.subheading %>
                            </p>

                            <div class="timeline-wrap space-y-6">
                                <% t.lovart_beta_page.timeline.items.forEach(function(item, i) { %>
                                    <div class="flex items-start">
                                        <span
                                            class="tl-dot <%= item.date === 'Coming Soon' ? 'tl-dot-future' : 'bg-green-500' %>"></span>
                                        <div>
                                            <div class="flex flex-wrap items-center gap-2 mb-1">
                                                <span class="font-bold text-neutral-dark text-sm">
                                                    <%= item.phase %>
                                                </span>
                                                <span
                                                    class="text-xs <%= item.date === 'Coming Soon' ? 'text-cyan-500 bg-cyan-50' : 'text-gray-400 bg-gray-100' %> px-2 py-0.5 rounded-full">
                                                    <%= item.date %>
                                                </span>
                                            </div>
                                            <p class="text-sm text-neutral leading-relaxed">
                                                <%= item.desc %>
                                            </p>
                                        </div>
                                    </div>
                                    <% }) %>
                            </div>
                        </div>
                    </section>

                    <!-- AdSense 2 -->
                    <div class="container mx-auto px-4 max-w-4xl my-6">
                        <%- include('partials/adsense', { format: 'horizontal' }) %>
                    </div>

                    <!-- â•â•â• COMPARISON TABLE â•â•â• -->
                    <section class="container mx-auto px-4 max-w-4xl py-12">
                        <h2 class="text-2xl md:text-3xl font-bold text-neutral-dark mb-8 text-center">
                            <%= t.lovart_beta_page.comparison.heading %>
                        </h2>
                        <div class="overflow-x-auto rounded-xl border border-gray-200 shadow-sm">
                            <table class="compare-table">
                                <thead>
                                    <tr>
                                        <th class="text-left"></th>
                                        <th>Beta Version</th>
                                        <th>âœ… Full Version (Now)</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% t.lovart_beta_page.comparison.rows.forEach(function(row, i) { %>
                                        <tr class="<%= i % 2 === 0 ? 'bg-white' : 'bg-gray-50/50' %>">
                                            <td>
                                                <%= row.feature %>
                                            </td>
                                            <td>
                                                <%= row.beta %>
                                            </td>
                                            <td>
                                                <%= row.full %>
                                            </td>
                                        </tr>
                                        <% }) %>
                                </tbody>
                            </table>
                        </div>
                        <div class="text-center mt-6">
                            <a href="<%= link('lovart-nano-banana') %>"
                                class="inline-flex items-center gap-2 text-primary font-semibold text-sm hover:underline">
                                Learn more about Nano Banana & Seedence 2.0 â†’
                            </a>
                        </div>
                    </section>

                    <!-- â•â•â• WHO SEARCHES FOR lovart beta â•â•â• -->
                    <section class="bg-green-50 py-10">
                        <div class="container mx-auto px-4 max-w-3xl">
                            <h2 class="text-xl md:text-2xl font-bold text-neutral-dark mb-4 text-center">
                                Who Is Still Searching for "lovart beta"?
                            </h2>
                            <div class="prose max-w-none text-neutral text-sm leading-relaxed space-y-3">
                                <p>The search query <strong>lovart beta</strong> continues to generate traffic
                                    because many users discovered Lovart during its invite-only phase and are looking to
                                    return or refer friends. When the <strong>lovart beta</strong> was active, access
                                    was gated by invitation codes and waitlists. The <strong>lovart beta</strong>
                                    experience was deliberately limited to maintain performance and gather structured
                                    feedback.</p>
                                <p>Since the <strong>lovart beta</strong> concluded, the product has improved
                                    dramatically. The Nano Banana model â€” unavailable during the <strong>lovart ai
                                        beta</strong> era â€” now powers all image generation, delivering photorealistic
                                    quality that far surpasses what beta testers experienced. Brand Kit and AI Logo
                                    Generator features were also absent in the <strong>lovart beta</strong>, and are
                                    now fully available on all plans.</p>
                                <p>If you are searching for the <strong>lovart beta</strong> program, the good news
                                    is simple: the full product is now better than the beta ever was, and it is free to
                                    access without any special codes or waitlists. Sign up at lovart.ai and explore
                                    everything the post-beta Lovart platform has to offer.</p>
                            </div>
                        </div>
                    </section>


                    <div class="container mx-auto px-4 max-w-4xl my-6">
                        <%- include('partials/adsense', { format: 'square' }) %>
                    </div>

                    <!-- â•â•â• FAQ â•â•â• -->
                    <section class="container mx-auto px-4 max-w-3xl my-10">
                        <h2 class="text-2xl md:text-3xl font-bold text-neutral-dark mb-6 text-center">
                            <%= t.lovart_beta_page.faq.heading %>
                        </h2>
                        <div class="space-y-3">
                            <% t.lovart_beta_page.faq.items.forEach(function(faq) { %>
                                <details class="bg-white border border-neutral-light rounded-xl px-5 py-4">
                                    <summary
                                        class="flex justify-between items-start gap-3 font-semibold text-neutral-dark text-sm md:text-base">
                                        <h3 class="m-0 font-semibold text-left">
                                            <%= faq.q %>
                                        </h3>
                                        <span class="chevron text-primary flex-shrink-0 mt-0.5">â–¾</span>
                                    </summary>
                                    <p class="mt-3 text-neutral text-sm leading-relaxed border-t border-gray-100 pt-3">
                                        <%= faq.a %>
                                    </p>
                                </details>
                                <% }) %>
                        </div>
                    </section>

                    <!-- AdSense 4 -->
                    <div class="container mx-auto px-4 max-w-4xl my-6">
                        <%- include('partials/adsense', { format: 'horizontal' }) %>
                    </div>

                </main>
                <%- include('partials/footer') %>
                    <script>
                        const btn = document.getElementById('mobile-menu-button');
                        const menu = document.getElementById('mobile-menu');
                        if (btn && menu) btn.addEventListener('click', () => menu.classList.toggle('hidden'));
                        const yr = document.getElementById('currentYear');
                        if (yr) yr.textContent = new Date().getFullYear();
                    </script>
    </body>

    </html>