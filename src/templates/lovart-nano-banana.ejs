<!DOCTYPE html>
<% const breadcrumbItems=[{ name: t.nav.home, href: link('/') }, { name: 'Lovart Nano Banana' , href: canonicalUrl }];
    %>
    <html lang="<%= lang %>" class="scroll-smooth">

    <head>
        <%- include('partials/head') %>
            <meta charset="UTF-8">
            <meta name="viewport" content="width=device-width, initial-scale=1.0">
            <title>
                <%= t.seo.lovart_nano_banana.title %>
            </title>
            <meta name="description" content="<%= t.seo.lovart_nano_banana.description %>">
            <link rel="canonical" href="<%= canonicalUrl %>">
            <%- include('partials/hreflang') %>
                <link rel="stylesheet" href="/styles.css">
                <link rel="icon" href="/favicon.ico" type="image/x-icon">
                <script async
                    src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-3083296102953963"
                    crossorigin="anonymous"></script>
                <%- include('partials/analytics') %>
                    <%- include('partials/schema-breadcrumb', { breadcrumbItems }) %>
                        <script type="application/ld+json"><%- JSON.stringify({
        "@context": "https://schema.org",
        "@type": "FAQPage",
        "mainEntity": t.lovart_nano_banana_page.faq.items.map(function(f) {
            return { "@type": "Question", "name": f.q, "acceptedAnswer": { "@type": "Answer", "text": f.a } };
        })
    }) %></script>

                        <style>
                            /* Hero gradient */
                            .hero-nb {
                                background: linear-gradient(135deg, #1a0533 0%, #3b0764 40%, #4c1d95 70%, #1e1b4b 100%);
                            }

                            /* Gallery grid */
                            .gallery-grid {
                                display: grid;
                                grid-template-columns: 1fr;
                                gap: 1.25rem;
                            }

                            @media (min-width: 640px) {
                                .gallery-grid {
                                    grid-template-columns: repeat(2, 1fr);
                                }
                            }

                            @media (min-width: 1024px) {
                                .gallery-grid {
                                    grid-template-columns: repeat(3, 1fr);
                                }
                            }

                            .gallery-card {
                                background: #0f0f17;
                                border: 1px solid rgba(139, 92, 246, 0.2);
                                border-radius: 0.875rem;
                                overflow: hidden;
                                transition: transform 0.2s, box-shadow 0.2s;
                            }

                            .gallery-card:hover {
                                transform: translateY(-3px);
                                box-shadow: 0 12px 40px rgba(139, 92, 246, 0.25);
                            }

                            .prompt-box {
                                font-family: ui-monospace, 'Fira Code', monospace;
                                font-size: 0.72rem;
                                line-height: 1.5;
                                color: #a78bfa;
                                background: #0a0a12;
                                border-top: 1px solid rgba(139, 92, 246, 0.15);
                                padding: 0.75rem;
                                position: relative;
                            }

                            .copy-btn {
                                position: absolute;
                                top: 0.5rem;
                                right: 0.5rem;
                                font-size: 0.65rem;
                                font-weight: 700;
                                color: #7c3aed;
                                background: rgba(139, 92, 246, 0.12);
                                border: 1px solid rgba(139, 92, 246, 0.3);
                                border-radius: 0.25rem;
                                padding: 0.2rem 0.5rem;
                                cursor: pointer;
                                transition: background 0.15s;
                                white-space: nowrap;
                            }

                            .copy-btn:hover {
                                background: rgba(139, 92, 246, 0.25);
                            }

                            /* Plan table */
                            .plan-table {
                                width: 100%;
                                border-collapse: collapse;
                            }

                            .plan-table th,
                            .plan-table td {
                                padding: 0.6rem 0.75rem;
                                text-align: center;
                                font-size: 0.85rem;
                                border-bottom: 1px solid #e5e7eb;
                            }

                            .plan-table th {
                                background: #f5f3ff;
                                font-weight: 700;
                                color: #4c1d95;
                            }

                            .plan-table td:first-child {
                                text-align: left;
                                color: #374151;
                            }

                            .plan-table tr:last-child td {
                                border-bottom: none;
                            }

                            /* Seedence coming-soon */
                            .seedence-card {
                                background: linear-gradient(135deg, #0f0f1a 0%, #1a0a2e 100%);
                                border: 1px solid transparent;
                                background-clip: padding-box;
                                position: relative;
                                border-radius: 1.25rem;
                            }

                            .seedence-card::before {
                                content: '';
                                position: absolute;
                                inset: -1px;
                                background: linear-gradient(135deg, #7c3aed, #06b6d4, #7c3aed);
                                border-radius: 1.25rem;
                                z-index: -1;
                                animation: borderSpin 4s linear infinite;
                                background-size: 200% 200%;
                            }

                            @keyframes borderSpin {
                                0% {
                                    background-position: 0% 50%;
                                }

                                50% {
                                    background-position: 100% 50%;
                                }

                                100% {
                                    background-position: 0% 50%;
                                }
                            }

                            .pulse-dot {
                                width: 0.625rem;
                                height: 0.625rem;
                                background: #06b6d4;
                                border-radius: 9999px;
                                display: inline-block;
                                animation: pulse 1.5s ease-in-out infinite;
                            }

                            @keyframes pulse {

                                0%,
                                100% {
                                    opacity: 1;
                                    transform: scale(1);
                                }

                                50% {
                                    opacity: 0.4;
                                    transform: scale(0.6);
                                }
                            }

                            /* FAQ */
                            details summary {
                                cursor: pointer;
                                list-style: none;
                            }

                            details summary::-webkit-details-marker {
                                display: none;
                            }

                            details[open] summary {
                                color: #7c3aed;
                            }

                            .chevron {
                                transition: transform 0.2s;
                                display: inline-block;
                            }

                            details[open] .chevron {
                                transform: rotate(180deg);
                            }
                        </style>
    </head>

    <body class="bg-neutral-light text-neutral-dark antialiased">

        <%- include('partials/header') %>
            <%- include('partials/breadcrumb', { breadcrumbItems }) %>

                <main>

                    <!-- ‚ïê‚ïê‚ïê HERO ‚ïê‚ïê‚ïê -->
                    <section class="hero-nb pt-14 pb-20 lg:pt-24 lg:pb-28 relative overflow-hidden">
                        <!-- Decorative blobs -->
                        <div
                            class="absolute top-0 left-1/4 w-96 h-96 bg-yellow-400/10 rounded-full blur-3xl pointer-events-none">
                        </div>
                        <div
                            class="absolute bottom-0 right-1/4 w-80 h-80 bg-purple-600/20 rounded-full blur-3xl pointer-events-none">
                        </div>

                        <div class="container mx-auto px-4 max-w-6xl relative">
                            <div class="grid lg:grid-cols-2 gap-10 items-center">
                                <div>
                                    <span
                                        class="inline-flex items-center gap-2 py-1 px-3 rounded-full bg-yellow-400/15 text-yellow-300 text-sm font-semibold mb-5 border border-yellow-400/30">
                                        <span class="pulse-dot"></span>
                                        <%= t.lovart_nano_banana_page.hero.badge %>
                                    </span>
                                    <h1
                                        class="text-3xl md:text-4xl lg:text-5xl font-bold text-white mb-4 leading-tight">
                                        <%= t.lovart_nano_banana_page.hero.title %>
                                    </h1>
                                    <p class="text-base md:text-lg text-purple-200 mb-8 leading-relaxed max-w-lg">
                                        <%= t.lovart_nano_banana_page.hero.subtitle %>
                                    </p>
                                    <div class="flex flex-col sm:flex-row gap-3">
                                        <a href="<%= link('lovart-free') %>"
                                            class="inline-flex items-center justify-center px-7 py-3.5 bg-yellow-400 text-purple-900 font-bold rounded-xl shadow-lg hover:bg-yellow-300 transition-all text-base">
                                            üçå <%= t.lovart_nano_banana_page.hero.cta_primary %> ‚Üí
                                        </a>
                                        <a href="<%= link('lovart-pricing') %>"
                                            class="inline-flex items-center justify-center px-7 py-3.5 bg-white/10 text-white font-semibold rounded-xl border border-white/20 hover:bg-white/20 transition-all text-base">
                                            <%= t.lovart_nano_banana_page.hero.cta_secondary %>
                                        </a>
                                    </div>
                                </div>
                                <div class="flex justify-center lg:justify-end">
                                    <img src="/images/nano-banana/hero.png"
                                        alt="Google Nano Banana AI model concept ‚Äî lovart nano banana powered art generation"
                                        class="w-64 sm:w-72 lg:w-80 drop-shadow-2xl rounded-2xl" width="320"
                                        height="320" loading="eager">
                                </div>
                            </div>
                        </div>
                    </section>

                    <!-- AdSense 1 (below hero) -->
                    <div class="container mx-auto px-4 max-w-5xl my-6">
                        <%- include('partials/adsense', { format: 'horizontal' }) %>
                    </div>

                    <!-- ‚ïê‚ïê‚ïê GALLERY ‚ïê‚ïê‚ïê -->
                    <section class="bg-gray-950 py-16">
                        <div class="container mx-auto px-4 max-w-6xl">
                            <h2 class="text-2xl md:text-3xl font-bold text-white mb-3 text-center">
                                <%= t.lovart_nano_banana_page.gallery.heading %>
                            </h2>
                            <p class="text-purple-300 text-center mb-10 max-w-2xl mx-auto text-sm leading-relaxed">
                                <%= t.lovart_nano_banana_page.gallery.subheading %>
                            </p>

                            <!-- Keyword paragraph for SEO -->
                            <p class="text-purple-400/60 text-xs text-center mb-8 max-w-3xl mx-auto leading-relaxed">
                                The <strong class="text-purple-300">Lovart Nano Banana</strong> model redefines what AI
                                image generation can achieve. Whether you're using <strong
                                    class="text-purple-300">Lovart Nano Banana</strong> for product shots, fantasy art,
                                or architectural renders, the results speak for themselves. Powered by Google's <strong
                                    class="text-purple-300">Nano Banana</strong> technology, every generation on Lovart
                                delivers photorealism that was previously impossible with standard AI models.
                            </p>

                            <div class="gallery-grid">
                                <% t.lovart_nano_banana_page.gallery.items.forEach(function(item, idx) { %>

                                    <% if (idx===2) { %>
                                        <!-- AdSense 2 ‚Äî mid-gallery (full-width display slot) -->
                                        <div class="sm:col-span-2 lg:col-span-3 my-2">
                                            <%- include('partials/adsense', { format: 'horizontal' }) %>
                                        </div>
                                        <% } %>

                                            <div class="gallery-card">
                                                <img src="/images/nano-banana/gallery-<%= idx + 1 %>.png"
                                                    alt="Lovart Nano Banana AI generated: <%= item.caption %>"
                                                    class="w-full aspect-square object-cover" width="400" height="400"
                                                    loading="lazy">
                                                <div class="prompt-box">
                                                    <button class="copy-btn" data-prompt="<%= item.prompt %>"
                                                        onclick="copyPrompt(this)">
                                                        <%= t.lovart_nano_banana_page.gallery.copy_label %>
                                                    </button>
                                                    <p class="pr-16 leading-relaxed">
                                                        <%= item.prompt %>
                                                    </p>
                                                </div>
                                            </div>

                                            <% }) %>
                            </div>
                        </div>
                    </section>

                    <!-- ‚ïê‚ïê‚ïê HOW TO ACCESS ‚ïê‚ïê‚ïê -->
                    <section class="bg-white py-16 my-2">
                        <div class="container mx-auto px-4 max-w-4xl">
                            <h2 class="text-2xl md:text-3xl font-bold text-neutral-dark mb-2 text-center">
                                <%= t.lovart_nano_banana_page.access.heading %>
                            </h2>
                            <p class="text-neutral text-center mb-8 max-w-xl mx-auto text-sm leading-relaxed">
                                <%= t.lovart_nano_banana_page.access.subheading %>
                            </p>

                            <!-- Plan comparison table -->
                            <div class="overflow-x-auto rounded-xl border border-gray-200 mb-8 shadow-sm">
                                <table class="plan-table">
                                    <thead>
                                        <tr>
                                            <% t.lovart_nano_banana_page.access.table_header.forEach(function(h) { %>
                                                <th
                                                    class="<%= h === t.lovart_nano_banana_page.access.table_header[0] ? 'text-left' : '' %>">
                                                    <%= h %>
                                                </th>
                                                <% }) %>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% t.lovart_nano_banana_page.access.table.forEach(function(row, i) { %>
                                            <tr class="<%= i % 2 === 0 ? 'bg-white' : 'bg-gray-50' %>">
                                                <td class="font-medium">
                                                    <%= row.feature %>
                                                </td>
                                                <td>
                                                    <%= row.free %>
                                                </td>
                                                <td>
                                                    <%= row.basic %>
                                                </td>
                                                <td class="text-primary font-semibold">
                                                    <%= row.pro %>
                                                </td>
                                            </tr>
                                            <% }) %>
                                    </tbody>
                                </table>
                            </div>

                            <div class="text-center">
                                <a href="<%= link('lovart-pricing') %>"
                                    class="inline-flex items-center justify-center w-full sm:w-auto px-8 py-4 bg-primary text-white text-base font-bold rounded-xl shadow-lg hover:bg-primary-dark transition-all">
                                    <%= t.lovart_nano_banana_page.access.cta %> ‚Üí
                                </a>
                                <p class="text-neutral text-xs mt-3">
                                    <%= t.lovart_nano_banana_page.access.note %>
                                </p>
                            </div>
                        </div>
                    </section>

                    <!-- AdSense 3 -->
                    <div class="container mx-auto px-4 max-w-4xl my-6">
                        <%- include('partials/adsense', { format: 'square' }) %>
                    </div>

                    <!-- ‚ïê‚ïê‚ïê SEEDENCE 2.0 COMING SOON ‚ïê‚ïê‚ïê -->
                    <section class="py-16 bg-gray-950">
                        <div class="container mx-auto px-4 max-w-4xl">
                            <div class="seedence-card p-8 md:p-12 relative">
                                <div class="flex items-center gap-2 mb-4">
                                    <span class="pulse-dot"></span>
                                    <span class="text-cyan-400 text-xs font-bold uppercase tracking-widest">
                                        <%= t.lovart_nano_banana_page.seedence.badge %>
                                    </span>
                                </div>
                                <h2 class="text-2xl md:text-3xl font-bold text-white mb-4">
                                    <%= t.lovart_nano_banana_page.seedence.heading %>
                                </h2>
                                <p class="text-gray-300 text-sm md:text-base leading-relaxed mb-6 max-w-2xl">
                                    <%= t.lovart_nano_banana_page.seedence.desc %>
                                </p>
                                <ul class="grid sm:grid-cols-2 gap-3 mb-8">
                                    <% t.lovart_nano_banana_page.seedence.bullets.forEach(function(b) { %>
                                        <li class="flex items-start gap-2 text-sm text-gray-200">
                                            <span class="text-cyan-400 flex-shrink-0 mt-0.5">‚óÜ</span>
                                            <span>
                                                <%= b %>
                                            </span>
                                        </li>
                                        <% }) %>
                                </ul>
                                <a href="<%= link('lovart-pricing') %>"
                                    class="inline-flex items-center justify-center px-7 py-3.5 bg-gradient-to-r from-purple-600 to-cyan-500 text-white font-bold rounded-xl shadow-lg hover:opacity-90 transition-all text-base">
                                    <%= t.lovart_nano_banana_page.seedence.cta %> ‚Üí
                                </a>
                            </div>
                        </div>
                    </section>

                    <!-- ‚ïê‚ïê‚ïê FAQ ‚ïê‚ïê‚ïê -->
                    <section class="container mx-auto px-4 max-w-3xl my-14">
                        <p class="text-neutral text-center text-sm leading-relaxed mb-8 max-w-2xl mx-auto">
                            Users searching for <strong>lovart nano banana</strong> often wonder how this Google-powered
                            model compares to standard AI generators. The <strong>Nano Banana</strong> model on Lovart
                            is not a third-party integration ‚Äî it is a deeply embedded, first-class AI generation option
                            available to all paid Lovart subscribers via the Unlimited Slow Generations feature.
                        </p>
                        <h2 class="text-2xl md:text-3xl font-bold text-neutral-dark mb-6 text-center">
                            <%= t.lovart_nano_banana_page.faq.heading %>
                        </h2>
                        <div class="space-y-3">
                            <% t.lovart_nano_banana_page.faq.items.forEach(function(faq) { %>
                                <details class="bg-white border border-neutral-light rounded-xl px-5 py-4">
                                    <summary
                                        class="flex justify-between items-start gap-3 font-semibold text-neutral-dark text-sm md:text-base">
                                        <h3 class="m-0 font-semibold text-left">
                                            <%= faq.q %>
                                        </h3>
                                        <span class="chevron text-primary flex-shrink-0 mt-0.5">‚ñæ</span>
                                    </summary>
                                    <p class="mt-3 text-neutral text-sm leading-relaxed border-t border-gray-100 pt-3">
                                        <%= faq.a %>
                                    </p>
                                </details>
                                <% }) %>
                        </div>
                    </section>

                    <!-- AdSense 4 -->
                    <div class="container mx-auto px-4 max-w-4xl my-6">
                        <%- include('partials/adsense', { format: 'horizontal' }) %>
                    </div>

                    <!-- ‚ïê‚ïê‚ïê FINAL CTA ‚ïê‚ïê‚ïê -->
                    <section class="hero-nb py-16 mt-2">
                        <div class="container mx-auto px-4 text-center max-w-2xl">
                            <h2 class="text-2xl md:text-3xl font-bold text-white mb-3">
                                <%= t.lovart_nano_banana_page.cta.title %>
                            </h2>
                            <p class="text-base text-purple-200 max-w-lg mx-auto mb-7">
                                <%= t.lovart_nano_banana_page.cta.desc %>
                            </p>
                            <a href="<%= link('lovart-free') %>"
                                class="inline-flex items-center justify-center w-full sm:w-auto px-8 py-4 bg-yellow-400 text-purple-900 text-lg font-bold rounded-xl shadow-lg hover:bg-yellow-300 transition-all">
                                üçå <%= t.lovart_nano_banana_page.cta.button %> ‚Üí
                            </a>
                        </div>
                    </section>

                </main>

                <%- include('partials/footer') %>

                    <script>
                        const btn = document.getElementById('mobile-menu-button');
                        const menu = document.getElementById('mobile-menu');
                        if (btn && menu) btn.addEventListener('click', () => menu.classList.toggle('hidden'));
                        const yr = document.getElementById('currentYear');
                        if (yr) yr.textContent = new Date().getFullYear();

                        function copyPrompt(el) {
                            const prompt = el.dataset.prompt;
                            navigator.clipboard.writeText(prompt).then(() => {
                                const orig = el.textContent;
                                el.textContent = el.closest('.prompt-box')?.previousElementSibling?.querySelector('[data-copied]')?.textContent || 'Copied!';
                                setTimeout(() => el.textContent = orig, 2000);
                            });
                        }
                    </script>

    </body>

    </html>